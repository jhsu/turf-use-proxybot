{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "target": "ES2015",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "declaration": true,
    "paths": {
      "turf-use-proxybot": [
        "src"
      ],
    },
  },
  "include": [
    "src",
    "demos/geographer/main.tsx"
  ],
  "exclude": [
    "node_modules",
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./examples/geographer/tsconfig.json"
    }
  ]
}